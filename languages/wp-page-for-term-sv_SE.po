msgid "Is page for term"
msgstr "Ersätt term-arkiv"

#: source/php/AcfFields/php/wp-page-for-term-term-fields.php:8
#: source/php/AcfFields/php/wp-page-for-term-term-fields.php:12
msgid "Page for term"
msgstr "Sida för term"

#: source/php/AcfFields/php/wp-page-for-term-term-fields.php:16
msgid "Redirect this term to the selected page."
msgstr "Omdirigera denna term till den valda sidan."

#: source/php/AcfFields/php/wp-page-for-term-page-fields.php:12
msgid "Term"
msgstr "Term"

#: source/php/AcfFields/php/wp-page-for-term-page-fields.php:16
